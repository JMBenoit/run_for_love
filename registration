    <script>
      function myMap() {
        latLng = new google.maps.LatLng(42.0626958,-71.1644764)
        var mapOptions = {
          center: latLng,
          zoom: 15,
          mapTypeId: google.maps.MapTypeId.ROADMAP
        }
        var map = new google.maps.Map(document.getElementById("map"), mapOptions);
        var marker = new google.maps.Marker({
          position: latLng,
          title:"Borderland State Park"
        });
        marker.setMap(map);
      }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDgaLiLcLzqifvfLlBCRN9zNfqCupWC9Ek&callback=myMap"></script>
